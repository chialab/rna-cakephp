{
    "name": "chialab/rna",
    "description": "Chialab/Rna plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "~4.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Chialab\\Rna\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chialab\\Rna\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
